clearsnippets :f fun proto

snippet :f "Object Method"
${1:name}: function (${3:arg}) {
	$0
}${10:,}
endsnippet

snippet proto "Prototype (proto)"
${1:class_name}.prototype.${2:name} = function (${3:}) {
	${0:}
};
endsnippet

snippet fun "function (fun)"
function ${1:name}(${2:}) {
    ${0:}
}
endsnippet

snippet o "function (fun)"
${1:object} = {
    ${0:}
}
endsnippet
